	<groupId>com.cntt.exam</groupId>
	<artifactId>report-service</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>report-service</name>
	<description>Demo project for Spring Boot</description>
	<url/>
	<licenses>
		<license/>
